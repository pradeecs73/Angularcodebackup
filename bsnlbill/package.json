{
  "name": "SimaticLiveLink-UI",
  "version": "0.0.0",
  "scripts": {
    "ng": "./node_modules/.bin/ng",
    "start": "./node_modules/.bin/ng serve",
    "build": "./node_modules/.bin/ng build",
    "test": "./node_modules/.bin/ng test --browsers=Chrome",
    "test-headless": "./node_modules/.bin/ng test --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "e2e": "./node_modules/.bin/ng e2e",
    "clean": "npm run clean:build && npm run clean:modules && npm run clean:archives",
    "clean:build": "rm -rf server/dist/data",
    "clean:modules": "rm -rf node_modules",
    "clean:archives": "rm -rf ../../../archives/ConnectionEditor/frontend",
    "build:prebuild": "npm run clean && npm install",
    "build:prod": "./node_modules/.bin/ng build --configuration production --aot"
  },
